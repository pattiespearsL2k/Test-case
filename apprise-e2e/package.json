{
    "name": "lumen-apprise-e2e",
    "version": "1.0.0",
    "description": "Lumen Apprise E2E",
    "main": "index.js",
    "scripts": {
        "test": "npx cypress open --browser chrome --env EMAIL=admin@here.com,PASSWORD=abc123,API_URL=https://apprise-api.dev.dgnx.io --config baseUrl=https://apprise-v2.dev.dgnx.io/",
        "format": "prettier --write \"cypress/**/*.ts\"",
        "check": "prettier --check \"cypress/**/*.ts\"",
        "lint": "./node_modules/.bin/eslint ./ --ext .tsx,.ts",
        "cypress:open": "cypress open",
        "cypress:run": "cypress run",
        "cypress:run:record": "CYPRESS_API_URL=\"http://cypress.dgnx.io:1234/\" cy2 run --parallel --record --key test --ci-build-id e2e-$(date \"+%s\") --browser chrome  --headed",
        "cypress:run:chrome": "cypress run --browser chrome",
        "cypress:run:headed": "cypress run --headed"
    },
    "devDependencies": {
        "@types/chance": "1.1.3",
        "@types/node": "17.0.21",
        "@typescript-eslint/parser": "5.14.0",
        "chai-sorted": "0.2.0",
        "cypress-should-really": "1.6.0",
        "eslint": "8.9.0",
        "eslint-plugin-cypress": "2.12.1",
        "eslint-plugin-jest-formatting": "3.1.0",
        "gmail-tester": "1.3.5",
        "husky": "7.0.4",
        "typescript": "4.3.2",
        "webpack": "5.38.1"
    },
    "dependencies": {
        "@cypress/webpack-preprocessor": "5.10.0",
        "@types/pluralize": "0.0.29",
        "chance": "1.1.8",
        "country-code-lookup": "0.0.20",
        "cy2": "2.0.0",
        "cypress": "9.7.0",
        "cypress-downloadfile": "1.2.2",
        "cypress-file-upload": "5.0.8",
        "cypress-real-events": "1.7.0",
        "cypress-xpath": "1.6.2",
        "exceljs": "4.3.0",
        "http-status-codes": "2.1.4",
        "jimp": "0.16.1",
        "moment": "2.29.1",
        "pluralize": "8.0.0",
        "prettier": "^2.2.1",
        "qrcode-reader": "1.0.4",
        "ts-loader": "9.2.6"
    }
}
